{
  "name": "yamlify-object",
  "version": "3.1.0",
  "description": "Stringify object/array with yaml syntax",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:esm && npm run build:cjs && npm run build:umd && npm run build:packages",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json && node scripts/write-types-extensions.cjs",
    "build:umd": "webpack",
    "build:packages": "node scripts/write-subpackage-json.cjs",
    "prepublish": "npm run lint && npm test && npm run build && attw --pack .",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "jest test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eugeny-dementev/yamlify-object.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "formatter",
    "console",
    "log",
    "yaml",
    "beauty",
    "stringify",
    "yamlify"
  ],
  "author": {
    "name": "Eugeny Dementev",
    "url": "http://github.com/eugeny-dementev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eugeny-dementev/yamlify-object/issues"
  },
  "homepage": "https://github.com/eugeny-dementev/yamlify-object"
}
